{
    "manifest_version": 3,
    "name": "notion sidebar toggle (cmd+/)",
    "version": "1.0.0",
    "content_scripts": [
      {
        "matches": [
          "https://www.notion.so/*",
          "https://notion.so/*",
          "https://calendar.notion.so/*"
        ],
        "js": ["content.js"],
        "run_at": "document_idle",
        "all_frames": true
      }
    ]
  }
  